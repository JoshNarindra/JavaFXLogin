<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<VBox alignment="CENTER" prefHeight="266.0" prefWidth="194.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxlogin.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>


    <Label fx:id="login" text="Login" />
   <TextField fx:id="username" promptText="Username" />
   <PasswordField fx:id="password" promptText="Password" />
    <Button fx:id="sign_in" onAction="#onSignInButtonClick" text="Sign in" />
    <Label fx:id="Text" />
</VBox>
